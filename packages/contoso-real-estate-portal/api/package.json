    "mongodb": "^4.7.0",
    "mongoose": "^6.5.3",
    "swagger-ui-dist": "^4.14.0",
    "winston": "^3.8.1",
    "winston-transport": "^4.5.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@azure/functions": "^1.2.3",
    "@types/swagger-ui-dist": "^3.30.1",
    "@types/yamljs": "^0.2.31",
    "azure-functions-core-tools": "^4.0.4736",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.0.0"
  }
