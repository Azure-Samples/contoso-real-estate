{
  "name": "contoso-real-estate",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "swa": "swa",
    "start": "concurrently npm:start:* --kill-others",
    "start:services": "docker compose up",
    "start:api": "npm run start --workspace=api",
    "start:website": "swa start",
    "test": "npm run test -ws --if-present",
    "build": "npm run build -ws --if-present",
    "format": "prettier --config .prettierrc.yml --ignore-path .prettierignore --write .",
    "format:check": "prettier --config .prettierrc.yml --ignore-path .prettierignore --check .",
    "lint": "npm run lint -ws --if-present",
    "lint:fix": "npm run lint:fix -ws --if-present",
    "clean": "rimraf \"packages/**/*.tsbuildinfo\"",
    "clean:install": "rimraf \"packages/**/node_modules\" \"node_modules\" && npm install"
  },
  "keywords": [],
  "author": "Microsoft",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^1.0.6",
    "concurrently": "^7.6.0",
    "git-commit-msg-linter": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
