{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "contoso-real-estate": {
      "appLocation": "packages/portal",
      "apiLocation": "packages/api",
      "outputLocation": "dist/contoso-app",
      "appBuildCommand": "npm run build",
      "apiBuildCommand": "npm run build --if-present",
      "run": "npm start",
      "appDevserverUrl": "http://localhost:4200"
    },
    "blog": {
      "appLocation": "packages/blog",
      "outputLocation": "packages/blog/.next",
      "appBuildCommand": "npm run build",
      "run": "npm run dev",
      "appDevserverUrl": "http://localhost:4201",
      "apiLocation": "packages/blog-cms",
      "apiBuildCommand": "npm run build --if-present",
      "apiRunCommand": "npm run develop",
      "apiDevserverUrl": "http://localhost:1337"
    }
  }
}
